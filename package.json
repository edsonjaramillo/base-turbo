{
  "name": "laundrybasketplus",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean:next": "find . -name '.next' -type d -prune -exec rm -rf '{}' +",
    "clean:node_modules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "clean:turbo": "find . -name '.turbo' -type d -prune -exec rm -rf '{}' +",
    "format": "prettier --write . --cache --ignore-path .prettierignore --config .prettierrc",
    "kill": "npx kill-port 3000 3001",
    "lint": "turbo run lint"
  },
  "dependencies": {
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.0",
    "next": "14.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "2.2.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/node": "20.11.20",
    "@types/react": "18.2.59",
    "autoprefixer": "10.4.17",
    "postcss": "8.4.35",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.3",
    "tailwindcss": "3.4.1",
    "turbo": "1.12.4",
    "typescript": "5.3.3"
  },
  "packageManager": "pnpm@8.7.6"
}
